@model IEnumerable<WebApplication2.Models.Attendance>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
<br />
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.courseCodeFK.courseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stu.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimetableIdFK.time)
